#################################################################

<https://braine.eccenca.dev/vocabulary/itops#pod/spec/container/image> rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Container ;
                   rdfs:range xsd:StringValueType ;
                   rdfs:isDefinedBy : ;
                   dct:issued "2022-06-28"^^xsd:date ;
                   rdfs:comment "Container image name. More info: https://kubernetes.io/docs/concepts/containers/images This field is optional to allow higher level config management to default or override container images in workload controllers like Deployments and StatefulSets."@en ;
                   rdfs:label "image"@en .

<https://braine.eccenca.dev/vocabulary/itops#pod/spec/container/imagePoolPolicy> rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Container ;
                   rdfs:range xsd:StringValueType ;
                   rdfs:isDefinedBy : ;
                   dct:issued "2022-06-28"^^xsd:date ;
                   rdfs:comment "Image pull policy. One of Always, Never, IfNotPresent. Defaults to Always if :latest tag is specified, or IfNotPresent otherwise. Cannot be updated. More info: https://kubernetes.io/docs/concepts/containers/images#updating-images."@en ;
                   rdfs:label "imagePoolPolicy"@en .

<https://braine.eccenca.dev/vocabulary/itops#pod/spec/container/terminationMessagePath> rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Container ;
                   rdfs:range xsd:StringValueType ;
                   rdfs:isDefinedBy : ;
                   dct:issued "2022-06-28"^^xsd:date ;
                   rdfs:comment "Optional: Path at which the file to which the container's termination message will be written is mounted into the container's filesystem. Message written is intended to be brief final status, such as an assertion failure message. Will be truncated by the node if greater than 4096 bytes. The total message length across all containers will be limited to 12kb. Defaults to /dev/termination-log. Cannot be updated."@en ;
                   rdfs:label "terminationMessagePath"@en .

<https://braine.eccenca.dev/vocabulary/itops#pod/spec/container/terminationMessagePolicy> rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Container ;
                   rdfs:range xsd:StringValueType ;
                   rdfs:isDefinedBy : ;
                   dct:issued "2022-06-28"^^xsd:date ;
                   rdfs:comment "Indicate how the termination message should be populated. File will use the contents of terminationMessagePath to populate the container status message on both success and failure. FallbackToLogsOnError will use the last chunk of container log output if the termination message file is empty and the container exited with an error. The log output is limited to 2048 bytes or 80 lines, whichever is smaller. Defaults to File. Cannot be updated."@en ;
                   rdfs:label "terminationMessagePolicy"@en .

<https://braine.eccenca.dev/vocabulary/itops#pod/spec/container/workingDir> rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Container ;
                   rdfs:range xsd:StringValueType ;
                   rdfs:isDefinedBy : ;
                   dct:issued "2022-06-28"^^xsd:date ;
                   rdfs:comment "Container's working directory. If not specified, the container runtime's default will be used, which might be configured in the container image. Cannot be updated."@en ;
                   rdfs:label "workingDir"@en .

<https://braine.eccenca.dev/vocabulary/itops#pod/spec/container/args> rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Container ;
                   rdfs:range xsd:StringValueType ;
                   rdfs:isDefinedBy : ;
                   dct:issued "2022-06-28"^^xsd:date ;
                   rdfs:comment "Arguments to the entrypoint. The container image's CMD is used if this is not provided. Variable references $(VAR_NAME) are expanded using the container's environment. If a variable cannot be resolved, the reference in the input string will be unchanged. Double $$ are reduced to a single $, which allows for escaping the $(VAR_NAME) syntax: i.e. '$(VAR_NAME)' will produce the string literal '$(VAR_NAME)'. Escaped references will never be expanded, regardless of whether the variable exists or not. Cannot be updated. More info: https://kubernetes.io/docs/tasks/inject-data-application/define-command-argument-container/#running-a-command-in-a-shell."@en ;
                   rdfs:label "args"@en .

<https://braine.eccenca.dev/vocabulary/itops#pod/spec/container/command> rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Container ;
                   rdfs:range xsd:StringValueType ;
                   rdfs:isDefinedBy : ;
                   dct:issued "2022-06-28"^^xsd:date ;
                   rdfs:comment "Entrypoint array. Not executed within a shell. The container image's ENTRYPOINT is used if this is not provided. Variable references $(VAR_NAME) are expanded using the container's environment. If a variable cannot be resolved, the reference in the input string will be unchanged. Double $$ are reduced to a single $, which allows for escaping the $(VAR_NAME) syntax: i.e. '$$(VAR_NAME)' will produce the string literal '$(VAR_NAME)'. Escaped references will never be expanded, regardless of whether the variable exists or not. Cannot be updated. More info: https://kubernetes.io/docs/tasks/inject-data-application/define-command-argument-container/#running-a-command-in-a-shell."@en ;
                   rdfs:label "command"@en .

<https://braine.eccenca.dev/vocabulary/itops#pod/spec/container/tty> rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Container ;
                   rdfs:range xsd:boolean ;
                   rdfs:isDefinedBy : ;
                   dct:issued "2022-06-28"^^xsd:date ;
                   rdfs:comment "Whether this container should allocate a TTY for itself, also requires 'stdin' to be true. Default is false."@en ;
                   rdfs:label "tty"@en .

<https://braine.eccenca.dev/vocabulary/itops#pod/spec/container/stdinOnce> rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Container ;
                   rdfs:range xsd:boolean ;
                   rdfs:isDefinedBy : ;
                   dct:issued "2022-06-28"^^xsd:date ;
                   rdfs:comment "Whether the container runtime should close the stdin channel after it has been opened by a single attach. When stdin is true the stdin stream will remain open across multiple attach sessions. If stdinOnce is set to true, stdin is opened on container start, is empty until the first client attaches to stdin, and then remains open and accepts data until the client disconnects, at which time stdin is closed and remains closed until the container is restarted. If this flag is false, a container processes that reads from stdin will never receive an EOF. Default is false."@en ;
                   rdfs:label "stdinOnce"@en .

<https://braine.eccenca.dev/vocabulary/itops#pod/spec/container/stdin> rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Container ;
                   rdfs:range xsd:boolean ;
                   rdfs:isDefinedBy : ;
                   dct:issued "2022-06-28"^^xsd:date ;
                   rdfs:comment "Whether this container should allocate a buffer for stdin in the container runtime. If this is not set, reads from stdin in the container will always result in EOF. Default is false."@en ;
                   rdfs:label "stdin"@en .

